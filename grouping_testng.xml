<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
    <groups>
        <run>
            <include name="Smoke"/>
            <include name="Sanity"/>
            <include name="Regression"/>
        </run>
    </groups>
    <test name="All Test Cases">
        <parameter name="browser" value="Chrome"/>
        <classes>
            <class name="org.qalegent.test.LoginPageTest"/>
            <class name="org.qalegent.test.HomePageTest"/>
            <class name="org.qalegent.test.ResetPagetest"/>
            <class name="org.qalegent.test.AddUsersPageTest"/>
            <class name="org.qalegent.test.UserManagementTest"/>
        </classes>
    </test>
</suite>